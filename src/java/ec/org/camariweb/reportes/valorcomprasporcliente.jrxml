	<queryString language="SQL">
		<![CDATA[SELECT
"public".cliente.ruc,
"public".cliente.nombre,
Sum("public".factura.total) AS total
FROM
"public".cliente
INNER JOIN "public".factura ON "public".factura.clienteid = "public".cliente."id"
GROUP BY
"public".cliente.nombre,
"public".cliente.ruc]]>
