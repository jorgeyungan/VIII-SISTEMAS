	<queryString>
		<![CDATA[SELECT
"public".producto.nombre,
Sum("public".dfacpro.cantidad) AS cantidad,
Sum("public".dfacpro.subtotal) AS "Valor total"
FROM
"public".dfacpro
INNER JOIN "public".producto ON "public".dfacpro.idproducto = "public".producto."id"
GROUP BY
"public".producto.nombre
ORDER BY
cantidad ASC
LIMIT 1]]>
	</queryString>
